	 <div class="container">
          <div class="panel panel-default"%>
           <div class="panel-body">

<div class="page-header">
   <h1>Lista de Reclamações</h1>
</div>
	<table id="reclamacao" class="display">
		<thead> 
			<tr>
				<td>Usuario</td>
				<td>Reclamação</td>
				<td>Descricao do convenio</td>
				<td>Numero Convenio</td>
				<td>Imagem</td>
			</tr>
		</thead>
	 
		<tbody>
			<% @reclamacoes.each do |reclamacao| %>

			<tr> 
				<td> <%= Usuario.find(reclamacao.usuario_id).nome %>  </td>
				<td> <%= reclamacao.descricao %>  </td>
				<td> <%= Convenio.find(reclamacao.convenio_id).situacaoconvenio %>  </td>
				<td><%=link_to raw(Convenio.find(reclamacao.convenio_id).numeroconvenio),
                           convenio_path(reclamacao.convenio_id) %> </td>
				<td> <%= image_tag reclamacao.foto(:medium) %>	</td>		

			</tr>
	 
	 
			<% end %>
		</tbody>
	</table>
	</div>
	</div>
	</div>

